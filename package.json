{
  "name": "vite-reactts-electron-starter",
  "version": "0.5.0",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "Maximilian St√ºmpfl"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
  },
  "productName": "vite-reactts-electron-starter",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \" npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder --ia32 --x64",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc"
  },
  "dependencies": {
    "@fullcalendar/core": "^5.9.0",
    "@fullcalendar/daygrid": "^5.9.0",
    "@fullcalendar/react": "^5.9.0",
    "@headlessui/react": "^1.4.0",
    "@supabase/supabase-js": "^1.22.3",
    "@tailwindcss/forms": "^0.3.3",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.1.8",
    "classnames": "^2.3.1",
    "electron-is-dev": "^2.0.0",
    "formik": "^2.2.9",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-query": "^3.21.0",
    "react-router-dom": "^5.2.0",
    "react-string-replace": "^0.4.4"
  },
  "devDependencies": {
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.0.0",
    "electron": "^13.1.7",
    "electron-builder": "^22.10.5",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.2.3",
    "vite": "^2.5.10",
    "vite-tsconfig-paths": "^3.3.13"
  },
  "build": {
    "productName": "Sonatrach Programme",
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "win": {
      "icon": "./assets/win-icon.png"
    }
  }
}